[Unit]
Description=Webhook para auto-deploy de Ruteo API
After=network.target

[Service]
Type=simple
User=riogas
WorkingDirectory=/home/riogas/ruteo
ExecStart=/usr/bin/webhook -hooks /home/riogas/ruteo/webhook.conf -port 9000 -verbose
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target
